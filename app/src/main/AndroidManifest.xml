<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.projetoavaliativo">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="DATABASE"
            android:value="projeto_avaliativo.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example.user.projetoavaliativo" />

        <activity
            android:name=".adocao.ListaAdocao"
            android:label="@string/title_activity_lista_adocao"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".adocao.CadastrarAdocao"
            android:label="@string/title_activity_cadastrar_adocao"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mausTratos.ListaMausTratos"
            android:label="@string/title_activity_lista_maus_tratos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mausTratos.CadastrarDenuncia"
            android:label="@string/title_activity_cadastrar_denuncia"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>